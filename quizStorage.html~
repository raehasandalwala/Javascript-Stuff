<script type="text/javascript" >
var disp,disp1,disp2,disp3,disp4,rad;

function getData()
{
disp = sessionStorage.getItem("key");
disp1 = sessionStorage.getItem("key1");
disp2 = sessionStorage.getItem("key2");
disp3 = sessionStorage.getItem("key3");
disp4 = sessionStorage.getItem("key4");
rad = sessionStorage.getItem("key5");
document.getElementById("display").innerHTML = disp;
document.getElementById("display1").innerHTML = disp1;
document.getElementById("display2").innerHTML = disp2;7
document.getElementById("display3").innerHTML = disp3;
document.getElementById("display4").innerHTML = disp4;
}

var rates, rate_value;
function getSelectedRadio(form, name) {
    rates = document.getElementsByName('cho');
for(var i = 0; i < rates.length; i++){
    if(rates[i].checked)
        rate_value = rates[i].id; 
             
 }
 validate();   
}

function validate(){
if(rate_value==rad)
alert("correct");
else
alert("incorrect");
}


</script>
